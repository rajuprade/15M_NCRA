<?xml version="1.0" encoding="UTF-8"?>
<groups template="">
	<group name="XScale">
		<controls>	 
			<row>
				<column>
					<control>	
						<name>Xscale_StartRangeLabel</name>
						<type>LABEL</type>    
						<cmdid>Xscale_StartRangeLabel</cmdid>						
						<label>XScale:</label>						
					</control>
				</column>
				<column>
					<control>	
						<name>Xscale_dropDown</name>
						<type>DropDown</type>    
						<cmdid>Xscale_dropDown</cmdid>
						<maxLength>5</maxLength>	
						<onchange>takeMinMaxValueForTrendPlot</onchange>
						<options>
							<option>
								<key>frontend:time</key>
								<value>frontend:time</value>								
								<selected>false</selected>
							</option>
							<option>
								<key>frontend:rf_det1</key>
								<value>frontend:rf_det1</value>
								<selected>false</selected>
							</option>
							<option>
								<key>frontend:rf_det2</key>
								<value>frontend:rf_det2</value>
								<selected>false</selected>
							</option>
							<option>
								<key>frontend:cb_temp1</key>
								<value>frontend:cb_temp1</value>
								<selected>false</selected>
							</option>
							<option>
								<key>frontend:cb_temp2</key>
								<value>frontend:cb_temp2</value>
								<selected>false</selected>
							</option>
							<option>
								<key>frontend:cb_temp3</key>
								<value>frontend:cb_temp3</value>
								<selected>false</selected>
							</option>
							<option>
								<key>frontend:laser_temp1</key>
								<value>frontend:laser_temp1</value>
								<selected>false</selected>
							</option>
							<option>
								<key>frontend:laser_temp2</key>
								<value>frontend:laser_temp2</value>
								<selected>false</selected>
							</option>
						</options>						
					</control>
				</column>
				
				<column>
					<control>	
						<name>Xscale_Min_Label</name>
						<type>LABEL</type>    
						<cmdid>Xscale_Min_Label</cmdid>						
						<label>Min Value:</label>						
					</control>
				</column>

				<column>
					<control class="double">	
						<name>Xscale_Min_Text</name>
						<type>Text</type>    
						<cmdid>Xscale_Min_Text</cmdid>
						<maxLength>5</maxLength>
						<value></value> 						
					</control>
				</column>
				
				<column>
					<control>	
						<name>Xscale_Max_Label</name>
						<type>LABEL</type>    
						<cmdid>Xscale_Max_Label</cmdid>						
						<label>Max Value:</label>						
					</control>
				</column>

				<column>
					<control class="double">	
						<name>Xscale_Max_Text</name>
						<type>Text</type>    
						<cmdid>Xscale_Max_Text</cmdid>
						<maxLength>5</maxLength>
						<value></value> 
					</control>
				</column>
				<column>
					<control>	
						<name>Xscale_IntervalLabel</name>
						<type>LABEL</type>    
						<cmdid>Xscale_IntervalLabel</cmdid>						
						<label>Plot Interval:</label>						
					</control>
				</column>

				<column>
					<control class="double">	
						<name>Xscale_Interval</name>
						<type>Text</type>    
						<cmdid>Xscale_Interval</cmdid>
						<maxLength>5</maxLength>	
						<value></value> 						
					</control>
				</column>
			</row> 
		</controls>
	</group>
	
