	<group name="Time Range">
		<controls>	 
			<row>
				<column>
					<control>	
						<name>Time_From_Label</name>
						<type>LABEL</type>    
						<cmdid>Time_From_Label</cmdid>						
						<label>Time From:</label>
					</control>
				</column>
				<column>
					<control readonly="readonly">	
						<name>Time_From_Text</name>
						<type>Text</type>    
						<cmdid>Time_From_Text</cmdid>
						<maxLength>5</maxLength>	
						<metaData>true</metaData>												
						<value></value> 						
					</control>
				</column>
				<column>
					<control>	
						<name>Time_From_Link</name>
						<type>a</type>
						<href>javascript: NewCssCal('Time_From_Text','mmddyyyy','dropdown',true,12)</href>    
						<cmdid>Time_From_Link</cmdid>						
						<metaData>true</metaData>
						<class>icoCalender</class>	
						<tooltip>Select from date</tooltip>
						<value></value> 						
					</control>
				</column>
				<column>
					<control>	
						<name>Time_To_Label</name>
						<type>LABEL</type>    
						<cmdid>Time_To_Label</cmdid>						
						<label>Time To:</label>													
					</control>
				</column>
				<column>
					<control readonly="readonly">	
						<name>Time_To_Text</name>
						<type>Text</type>    
						<cmdid>Time_To_Text</cmdid>
						<maxLength>5</maxLength>	
						<metaData>true</metaData>
						<value></value> 						
					</control>
				</column>
				<column>
					<control>	
						<name>Time_To_Link</name>
						<type>a</type>
						<href>javascript: NewCssCal('Time_To_Text','mmddyyyy','dropdown',true,12)</href>    
						<cmdid>Time_To_Link</cmdid>						
						<metaData>true</metaData>
						<class>icoCalender</class>
						<tooltip>Select to date</tooltip>
						<value></value> 
					</control>
				</column>
			</row>
		</controls>
	</group>
	
	<group name="Refresh Interval">
		<controls>	 
			<row>
				<column>
					<control>	
						<name>Refresh_Interval_Label</name>
						<type>LABEL</type>    
						<cmdid>Refresh_Interval_Label</cmdid>						
						<label>Refresh Interval(msec):</label>						
					</control>
				</column>
				<column>
					<control class="int">	
						<name>Refresh_Interval_Value</name>
						<type>Text</type>    
						<cmdid>Refresh_Interval_Value</cmdid>
						<maxLength>5</maxLength>	
						<metaData>true</metaData>
						<value></value> 
					</control>
				</column>
			</row>
		</controls>
	</group>
</groups>
	
